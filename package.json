{
  "name": "portal-ui",
  "version": "1.1.1",
  "description": "VibroSense Portal Front-end",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch' npm:backend:start",
    "clean-www": "rimraf target/classes/static/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/",
    "lint": "eslint . --ext .ts,.vue ",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue,java}\"",
    "serve": "npm run start --",
    "start": "npm run webapp:dev --",
    "start-tls": "npm run webapp:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "npm run vitest-run --",
    "test:watch": "npm run vitest",
    "vitest": "vitest",
    "vitest-run": "vitest --run --coverage",
    "watch": "concurrently npm:start npm:backend:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev --",
    "webapp:build:dev": "npm run webpack -- --mode development --env stats=minimal",
    "webapp:build:prod": "npm run webpack -- --mode production --env stats=minimal",
    "webapp:dev": "npm run webpack-dev-server -- --mode development --env stats=normal",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod --",
    "webapp:test": "npm run test --",
    "webpack": "webpack --config webpack/webpack.common.js",
    "webpack-dev-server": "webpack serve --config webpack/webpack.common.js"
  },
  "config": {
    "backend_port": 8080,
    "default_environment": "prod",
    "packaging": "jar"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/vue-fontawesome": "3.0.3",
    "@vue/compat": "3.3.4",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.3",
    "@vueuse/core": "10.2.1",
    "axios": "1.4.0",
    "bootstrap": "^5.3.1",
    "bootstrap-vue": "2.23.1",
    "browser-sync": "^2.29.3",
    "chart.js": "^4.4.0",
    "chroma-js": "^2.4.2",
    "dotenv": "^16.3.1",
    "vue": "3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-multiselect": "^2.0.8",
    "vue-router": "4.2.4",
    "vue3-html2pdf": "^1.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.2",
    "@types/node": "18.16.19",
    "@types/sinon": "10.0.15",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-v8": "0.34.3",
    "@vue/compiler-sfc": "3.3.4",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.4.0",
    "autoprefixer": "10.4.14",
    "axios-mock-adapter": "1.21.5",
    "browser-sync-webpack-plugin": "2.3.0",
    "concurrently": "8.2.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "eslint": "8.44.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "8.7.1",
    "happy-dom": "10.1.1",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "mini-css-extract-plugin": "2.7.6",
    "numeral": "2.0.6",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.3.3",
    "postcss-url": "10.1.3",
    "prettier": "2.8.8",
    "prettier-plugin-java": "2.2.0",
    "prettier-plugin-packagejson": "2.4.5",
    "rimraf": "5.0.1",
    "sass": "1.63.6",
    "sass-loader": "13.3.2",
    "sinon": "15.2.0",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "vitest": "0.34.3",
    "vitest-sonar-reporter": "0.4.1",
    "vue-loader": "17.2.2",
    "vue-style-loader": "4.1.3",
    "wait-on": "7.0.1",
    "webpack": "5.88.1",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  },
  "engines": {
    "node": ">=18.16.1",
    "npm": ">= 6.14.4"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
